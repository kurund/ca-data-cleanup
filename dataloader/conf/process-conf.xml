<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="Baseline" class="com.salesforce.dataloader.process.ProcessRunner" singleton="false">
        <description>Push Baseline data to Salesforce.</description>
        <property name="name" value="Baseline"/>
        <property name="configOverrideMap">
            <map>
                <entry key="sfdc.timeoutSecs" value="600"/>
                <entry key="sfdc.loadBatchSize" value="10"/>
                <entry key="sfdc.useBulkApi" value="true"/>
                <entry key="sfdc.bulkApiSerialMode" value="true"/>
                <entry key="sfdc.externalIdField" value="Bar_Code__c"/>
                <entry key="sfdc.entity" value="Contact"/>
                <entry key="process.operation" value="upsert"/>
                <entry key="process.mappingFile" value="mapping/baseline.sdl"/>
                <entry key="dataAccess.name" value="data/baseline.csv"/>
                <entry key="dataAccess.type" value="csvRead"/>
            </map>
        </property>
    </bean>
